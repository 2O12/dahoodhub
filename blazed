section1:addButton("Auto Stomp", function()
	venyx:Notify("Blazed", "Autostomp Enabled, Press Z To Enable/Disable")
	Toggle = false

	while wait(.5) do
		local mouse = game.Players.LocalPlayer:GetMouse()

		if Toggle == false then
			game.ReplicatedStorage.MainEvent:FireServer("Stomp")
			mouse.KeyDown:connect(function(k)
				if k == "z" then
					if Toggle == false then 
						Toggle = true
						print('Off')
					elseif Toggle == true then 
						print('On')
						Toggle = false
					end
				end
			end)
		end
	end
end)
