	game.Players.LocalPlayer.Backpack["[Knife]"].GripUp     = Vector3.new(-2, 0, 0)
	game.Players.LocalPlayer.Backpack["[Knife]"].Parent = game.Players.LocalPlayer.Character

	game.Players.LocalPlayer.Backpack["[Knife]"].GripPos     = Vector3.new(6, 0, 0)
	game.Players.LocalPlayer.Backpack["[Knife]"].GripForward     = Vector3.new(-1, 0, 0)
	game.Players.LocalPlayer.Backpack["[Knife]"].GripRight     = Vector3.new(1, 0, 0)
